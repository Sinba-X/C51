# EIDE 项目配置模板
# 复制此文件为 eide.yml 后，修改以下内容以匹配你的环境：
#   1. incList 中的 Keil C51 头文件路径
#   2. uploadConfigMap 中的 python 路径和 COM 端口

version: "4.1"
name: lesson3_lesson3_1
type: C51
deviceName: null
packDir: null
srcDirs: []
virtualFolder:
  name: <virtual_root>
  files: []
  folders:
    - name: lesson3/lesson3_1
      files:
        - path: src/lesson3/lesson3_1/STARTUP.A51
        - path: src/lesson3/lesson3_1/LED.c
      folders: []
dependenceList: []
outDir: build
miscInfo:
  uid: 2299258500f761a441ee416c0bd416ad
targets:
  Target 1:
    cppPreprocessAttrs:
      defineList: []
      incList:
        # >>> 修改为你本机的 Keil C51 安装路径 <<<
        - C:/Keil_v5/C51/INC
      libList: []
    excludeList: []
    toolchain: Keil_C51
    toolchainConfigMap:
      Keil_C51:
        options:
          version: 2
          afterBuildTasks: []
          asm-compiler: {}
          beforeBuildTasks: []
          c/cpp-compiler:
            optimization-level: level-8
            optimization-type: SPEED
          global:
            ram-mode: SMALL
            rom-mode: LARGE
          linker:
            misc-controls: ""
            output-format: elf
            remove-unused: true
    uploadConfigMap:
      Custom:
        bin: ""
        # >>> 修改 python 路径和 COM 端口 <<<
        commandLine: '"python" -m stcgal -p COM5 "${hexFile}"'
        eraseChipCommand: ""
      stcgal:
        # >>> 修改为你本机的 python 路径 <<<
        bin: python
        eepromImgPath: "null"
        extraOptions: -m stcgal
        options: .eide/stc.flash.json
    uploader: Custom
